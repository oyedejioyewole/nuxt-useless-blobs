{
  "name": "nuxt-useless-blobs",
  "version": "1.2.0",
  "description": "An easier way of using useless-blobs in your Nuxt project!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyedejioyewole/nuxt-useless-blobs.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm --filter docs dev",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare docs && nuxi prepare playground",
    "lint": "eslint . --fix",
    "prepack": "nuxt-module-build build",
    "release": "release-it",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "defu": "^6.1.4",
    "useless-blobs": "^1.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/kit": "^4.1.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.0",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "nuxt": "^4.1.0",
    "release-it": "^19.0.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}
